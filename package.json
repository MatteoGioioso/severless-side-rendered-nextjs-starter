    "clean:public": "rm -rf public/static/ && rm -rf public/_next/static && rm -rf functions/_next/",
    "build:public": "cpx \"functions/_next/static/**/*.*\" public/_next/static/  && cpx \"static/**/*.*\" public/static/",
